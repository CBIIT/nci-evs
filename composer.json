{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3",
        "drupal/asset_injector": "^2.4",
        "drupal/autologout": "^1.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/config_sync": "^2.0@beta",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9.5.5",
        "drupal/devel_entity_updates": "^3",
        "drupal/diff": "^1.1",
        "drupal/easy_install": "^10.5",
        "drupal/exclude_node_title": "^1.0@beta",
        "drupal/extlink": "^1.7",
        "drupal/google_analytics": "^4.0",
        "drupal/imce": "^2.4",
        "drupal/ldap": "^4.3",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/module_missing_message_fixer": "^2.0",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.3",
        "drupal/redirect_after_login": "^2.5",
        "drupal/security_review": "1.x-dev",
        "drupal/twig_xdebug": "^1.1",
        "drupal/views_fieldsets": "^3.3",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^10.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "extra": {
       "patches": {
               "drupal/redirect_after_login": {
               "Patch to fix login issue with redirect_after_login module": "https://www.drupal.org/files/issues/2022-07-26/redirect_after_login-3214949-107.patch"
               }
       },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
